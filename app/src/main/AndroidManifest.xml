<?xml version="1.0" encoding="utf-8"?>
<manifest
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
package="com.sb.tocaraul">
<uses-permission android:name="android.permission.INTERNET"/>


<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="TocaRaul"
    android:theme="@style/Theme.AppCompat.NoActionBar">

    <activity
        android:name="com.sb.tocaraul.activity.SplashScreenActivity"
        android:screenOrientation="portrait"
        android:exported="true"
        tools:ignore="LockedOrientationActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:name="com.sb.tocaraul.activity.PrincipalActivity"
        android:screenOrientation="portrait"
        tools:ignore="LockedOrientationActivity" />
    <activity android:name="com.sb.tocaraul.activity.Chat_Room"
        android:screenOrientation="portrait"
        tools:ignore="LockedOrientationActivity" />
    <activity android:name="com.sb.tocaraul.activity.ChatActivity"
        android:screenOrientation="portrait"
        tools:ignore="LockedOrientationActivity" />

</application>

</manifest>